<?xml version='1.0' encoding='UTF-8'?>
<!--  See src/resources/configuration/ReadMe.txt for how the configuration assembly works -->
<config>
   <extension-module>org.jboss.as.sunorb</extension-module>
   <subsystem xmlns="urn:jboss:domain:sunorb:1.0">
       <orb socket-binding="sunorb" ssl-socket-binding="sunorb-ssl">
           <initializers transactions="spec" security="on"/>
       </orb>
   </subsystem>
   <socket-binding name="sunorb" interface="unsecure" port="3528"/>
   <socket-binding name="sunorb-ssl" interface="unsecure" port="3529"/>
</config>